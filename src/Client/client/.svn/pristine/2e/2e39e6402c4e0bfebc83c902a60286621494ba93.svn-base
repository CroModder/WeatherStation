      <Label fx:id="lblExampleEmail" alignment="CENTER" layoutX="70.0" layoutY="327.0" prefHeight="15.0" prefWidth="182.0" text="example: jerkovic@th-wildau.de">
         <font>
            <Font size="10.0" />
         </font></Label>
      <ProgressBar fx:id="progressBar" layoutX="70.0" layoutY="225.0" prefHeight="14.0" prefWidth="182.0" progress="0.0" />
   </children>
